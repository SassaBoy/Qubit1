<section id="clients">
  <div class="container" data-aos="fade-up">
    <div class="section-header">
      <h2>Clients</h2>
      <p>Trusted by a diverse clientele, we deliver exceptional web development solutions for businesses of all sizes and industries.</p>
    </div>

    <div class="clients-slider swiper" data-aos="fade-up" data-aos-delay="100">
      <div class="swiper-wrapper align-items-center">
        <% if (clients.length > 0) { %>
          <!-- Clients found -->
          <% clients.forEach(client => { %>
            <div class="swiper-slide">
              <img src="/uploads/<%= client.logoPath %>" class="img-fluid" alt="<%= client.name %>">
            </div>
          <% }) %>
        <% } else { %>
          <!-- No clients found -->
          <div class="swiper-slide">
            <p>No clients available at the moment.</p>
          </div>
        <% } %>
      </div>
      <div class="swiper-pagination"></div>
    </div>
  </div>
</section>


